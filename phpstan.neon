parameters:
  level: 6
  paths:
    - src

  ignoreErrors:
    - "#Method [a-zA-Z0-9\\_\\\\:\\(\\)]+ has parameter \\$[a-zA-Z0-9_]+ with no value type specified in iterable type array#"
